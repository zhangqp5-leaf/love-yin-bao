{
  "name": "unicom-fe",
  "version": "0.1.0",
  "homepage": "https://loquatlo.gitee.io/love-yin-bao",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:js": "node_modules/eslint/bin/eslint.js --ext .js,.jsx ./src",
    "lint:style:fix": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:js:fix": "node_modules/eslint/bin/eslint.js --fix --ext .js,.jsx ./src",
    "lint:fix": "npm run lint:style:fix && npm run lint:js:fix",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:js:fix",
    "*.{css,less,scss,styl}": "npm run lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.12.3",
    "aplayer": "^1.10.1",
    "axios": "^0.21.1",
    "craco-less": "^1.17.1",
    "echarts": "^5.0.2",
    "echarts-for-react": "^3.0.0",
    "fs": "0.0.1-security",
    "gh-pages": "^3.1.0",
    "jsonp": "^0.2.1",
    "react": "^17.0.1",
    "react-aplayer": "^1.0.0",
    "react-bmapgl": "^0.1.4",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@craco/craco": "^6.1.1",
    "@ecomfe/eslint-config": "^4.0.0",
    "@ecomfe/stylelint-config": "^1.0.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/node-logger": "^6.1.18",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "babel-eslint": "^10.0.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "react-redux": "^7.2.3",
    "stylelint": "^13.0.0"
  }
}
